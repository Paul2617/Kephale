git init
git branch -M main
git add .
git commit -m "Premier"
git remote add origin https://github.com/Paul2617/Kephale.git
git push -u origin main


git push 

// metre en jour du serveur en locale
git pull

// suprimer un depo git 
git reset --soft HEAD~1
git log --oneline
rm -rf .git
ls -la | grep git
git status 2>&1 || echo "Le dépôt Git a été complètement supprimé"
// fin

// Metre a jour 
// Vérification de l'état du dépôt Git.
git status

// Un fichier est déjà en staging. Vérification des changements non suivis, puis commit et push.
git diff --cached 
git log --oneline -5

git commit -m "Mise à jour: ajout de commandes Git supplémentaires"

git push