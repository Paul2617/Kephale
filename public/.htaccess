# Forcer le type MIME pour JS
AddType application/javascript .js

# Ne pas rediriger le SW vers index.php (si tu utilises un routeur PHP)
RewriteCond %{REQUEST_URI} ^/js/sw\.js$ [NC]
RewriteRule .* - [L]

